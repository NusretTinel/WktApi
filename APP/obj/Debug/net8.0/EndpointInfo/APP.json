    "/optimize": {
      "post": {
        "tags": [
          "TrashBin"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OptimizationRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/OptimizationRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/OptimizationRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WktModel"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WktModel"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/WktModel"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "OptimizationRequest": {
        "required": [
          "cellSize",
          "existingBins",
          "minDistance",
          "newBinCount",
          "populationHeatmap"
        ],
        "type": "object",
        "properties": {
          "existingBins": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TrashBinInputDto"
            }
          },
          "populationHeatmap": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "number",
                "format": "double"
              }
            }
          },
          "cellSize": {
            "minimum": 0.1,
            "type": "number",
            "format": "double"
          },
          "newBinCount": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "minDistance": {
            "minimum": 0.1,
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "TrashBinInputDto": {
        "type": "object",
        "properties": {
          "wkt": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "WktModel": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "wkt": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}